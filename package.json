{
  "name": "turborepo-fullstack-starter",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "prettier": "^3.2.5",
    "turbo": "2.3.3",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.17"
}
